<section class="About">
	<h2 class="About__title">{{ about.title }}</h2>

	<picture class="About__picture">
		<source media="screen" srcset="{{ about.img.small }}">
		<img class="About__img" src="{{ about.img.small }}" alt="{{ about.img.alt }}">
	</picture>

	<div class="About__content rte">
		{{ about.content | safe }}
	</div>
</section>
