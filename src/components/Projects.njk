<section class="Projects">
	<h2>{{ projects.title }}</h2>

	<ul>
		{% for project in projects.items %}
			<li>
				<div>
					<img class="js-lazy-img" src="/assets/placeholder.png" data-srcset="https://placekitten.com/300/400" alt="cat">
				</div>
				<div>
					<h3>{{ project.title }}</h3>
					<p>{{ project.blurb }}</p>
				</div>
			</li>
		{% endfor %}
	</ul>
</section>
